{
  "manifest_version": 3,
  "name": "calendarius",
  "description": "Capture part of your screen and convert it into a Google Calendar event.",
  "options_ui": {
    "page": "src/pages/options/index.html"
  },
  "action": {
    "default_popup": "src/pages/popup/index.html",
    "default_icon": {
      "32": "icon-32.png"
    }
  },
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": ["scripting", "tabs", "activeTab", "storage", "identity"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ],
      "js": [
        "src/pages/content/index.tsx"
      ],
      "css": [
        "contentStyle.css"
      ]
    }
  ],
  "devtools_page": "src/pages/devtools/index.html",
  "web_accessible_resources": [
    {
      "resources": [
        "contentStyle.css",
        "icon-128.png",
        "icon-32.png"
      ],
      "matches": []
    }
  ],
  "background": {
    "service_worker": "../background/intex.ts"
  },
  "oauth2": {
  "client_id": "585735488383-ob7h2r0tgfkhr27d4me92747uoek38qt.apps.googleusercontent.com",
  "scopes": [
    "https://www.googleapis.com/auth/calendar.events"
  ]
  }
}
